<?xml version="1.0" encoding="UTF-8"?><BulkOperationMetadata templateName="addSpecimenAndAnnotation" batchSize="5">	<BulkOperationClass className="edu.wustl.catissuecore.domain.Specimen">			<referenceAssociation	className="edu.wustl.catissuecore.domain.SpecimenCollectionGroup" roleName="specimenCollectionGroup" >				<attribute name="name" csvColumnName="SCG name"/>		</referenceAssociation>			    	<containmentAssociation className="edu.wustl.catissuecore.domain.SpecimenCharacteristics"  roleName="specimenCharacteristics">				<attribute name="tissueSite" csvColumnName="Tissue Site"/>				<attribute name="tissueSide" csvColumnName="Tissue Side"/>		</containmentAssociation>				<containmentAssociation className="edu.wustl.catissuecore.domain.SpecimenPosition" roleName="specimenPosition" >				<attribute name="positionDimensionOne" csvColumnName="Position one" />				<attribute name="positionDimensionTwo" csvColumnName="Position two" />								<referenceAssociation	className="edu.wustl.catissuecore.domain.StorageContainer" roleName="storageContainer"  >					<attribute name="name" csvColumnName="Container name" />				</referenceAssociation>									</containmentAssociation>				<containmentAssociation className="edu.wustl.catissuecore.domain.deintegration.SpecimenRecordEntry" cardinality="*" roleName="specimenRecordEntryCollection" parentRoleName="specimen">			<attribute name="activityStatus" csvColumnName="activityStatus"/>													<containmentAssociation className="gov.nih.nci.dynext.pathology_specimen.SpecimenBaseSolidTissuePathologyAnnotation"	parentRoleName="specimenRecordEntry_SpecimenBaseSolidTissuePathologyAnnotation" cardinality="*" roleName="specimenBaseSolidTissuePathologyAnnotationCollection" >				<attribute name="comments" csvColumnName="Comments For SpecimenBaseSolidTissuePathologyAnnotation"/>				<containmentAssociation className="gov.nih.nci.dynext.pathology_specimen.SpecimenAdditionalFinding"	cardinality="*" roleName="additionalFinding" parentRoleName="specimenBaseSolidTissuePathologyAnnotation">										<attribute name="pathologicFinding" csvColumnName="Pathologic Finding" />									   	<containmentAssociation	className="gov.nih.nci.dynext.pathology_specimen.SpecimenDetails" parentRoleName="additionalFinding" cardinality="*" roleName="details" >						<attribute name="details" csvColumnName="Detail" />					</containmentAssociation>											</containmentAssociation>									<containmentAssociation	className="gov.nih.nci.dynext.pathology_specimen.SpecimenHistologicType" cardinality="*" roleName="histologicType" parentRoleName="specimenBaseSolidTissuePathologyAnnotation">					<attribute name="type" csvColumnName="Histoogic Type" />										<containmentAssociation	className="gov.nih.nci.dynext.pathology_specimen.SpecimenHistologicVariantType"	parentRoleName="histologicType" cardinality="*" roleName="histologicVariantType" >						<attribute name="otherHistologicType" csvColumnName="Histologic Variant Type" />					</containmentAssociation>						</containmentAssociation>							<containmentAssociation	className="gov.nih.nci.dynext.pathology_specimen.SpecimenHistologicGrade"	parentRoleName="specimenBaseSolidTissuePathologyAnnotation"  cardinality="*" roleName="histologicGrade">					<attribute name="gradingSystemName" csvColumnName="Grading System Name" />					<attribute name="grade" csvColumnName="Grade" />					<attribute name="otherGradingSystemName" csvColumnName="Other Grading System Name" />				</containmentAssociation>					</containmentAssociation>		</containmentAssociation>				<attribute name="label" csvColumnName="Label" />		<attribute name="barcode" csvColumnName="Barcode" />		<attribute name="createdOn" csvColumnName="Created On" />		<attribute name="activityStatus" csvColumnName="Activity Status" />		<attribute name="comment" csvColumnName="Comment" />		<attribute name="isAvailable" csvColumnName="Is Available" />		<attribute name="collectionStatus" csvColumnName="Collection Status" />		<attribute name="specimenClass" csvColumnName="Specimen Class" >			<discriminatorCollection name="Tissue" value="edu.wustl.catissuecore.domain.TissueSpecimen"/>			<discriminatorCollection name="Fluid" value="edu.wustl.catissuecore.domain.FluidSpecimen"/>			<discriminatorCollection name="Molecular" value="edu.wustl.catissuecore.domain.MolecularSpecimen"/>			<discriminatorCollection name="Cell" value="edu.wustl.catissuecore.domain.CellSpecimen"/>		</attribute>		<attribute name="specimenType" csvColumnName="Type" />		<attribute name="initialQuantity" csvColumnName="Initial Quantity" />		<attribute name="availableQuantity" csvColumnName="Available Quantity" />		<attribute name="lineage" csvColumnName="Lineage" updateBasedOn="false" />		<attribute name="pathologicalStatus" csvColumnName="Pathological Status" />			</BulkOperationClass></BulkOperationMetadata>
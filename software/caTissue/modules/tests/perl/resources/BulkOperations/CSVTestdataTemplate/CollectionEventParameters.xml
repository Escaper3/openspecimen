<?xml version="1.0" encoding="UTF-8"?>
<BulkOperationMetadata templateName="createCollectionEventParameters" batchSize="5">
	<BulkOperationClass className="edu.wustl.catissuecore.domain.CollectionEventParameters"
		 
		     parentRoleName="collectionEventParameters" >
		<referenceAssociation
			className="edu.wustl.catissuecore.domain.User"
			 
			  roleName="user"    >
				<attribute name="loginName" csvColumnName="User Login Name"   />
		</referenceAssociation>
		<containmentAssociation
			className="edu.wustl.catissuecore.domain.Specimen"
			 
			  roleName="specimen"   parentRoleName="collectionEventParameters" >
			<attribute name="label" csvColumnName="Specimen label"   />	
		</containmentAssociation>
		<attribute name="timestamp" csvColumnName="Date (MM-DD-YYYY HH:MM)"   />
		<attribute name="comment" csvColumnName="Comments"   />
		<attribute name="collectionProcedure" csvColumnName="collection Procedure"   />
		<attribute name="container" csvColumnName="container"   />
	</BulkOperationClass>
</BulkOperationMetadata>
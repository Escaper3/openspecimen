<?xml version="1.0" encoding="UTF-8"?><BulkOperationMetadata templateName="editSpecimenAndAnnotation" batchSize="5">	<BulkOperationClass className="edu.wustl.catissuecore.domain.Specimen">					<containmentAssociation className="edu.wustl.catissuecore.domain.deintegration.SpecimenRecordEntry" cardinality="*" roleName="specimenRecordEntryCollection" parentRoleName="specimen">			<attribute name="activityStatus" csvColumnName="activityStatus"/>													<containmentAssociation className="gov.nih.nci.dynext.pathology_specimen.SpecimenBaseSolidTissuePathologyAnnotation"	parentRoleName="specimenRecordEntry_SpecimenBaseSolidTissuePathologyAnnotation" cardinality="*" roleName="specimenBaseSolidTissuePathologyAnnotationCollection" >				<attribute name="comments" csvColumnName="Comments For SpecimenBaseSolidTissuePathologyAnnotation"/>				<containmentAssociation className="gov.nih.nci.dynext.pathology_specimen.SpecimenAdditionalFinding"	cardinality="*" roleName="additionalFinding" parentRoleName="specimenBaseSolidTissuePathologyAnnotation">										<attribute name="pathologicFinding" csvColumnName="Pathologic Finding" />									   	<containmentAssociation	className="gov.nih.nci.dynext.pathology_specimen.SpecimenDetails" parentRoleName="additionalFinding" cardinality="*" roleName="details" >						<attribute name="details" csvColumnName="Detail" />					</containmentAssociation>											</containmentAssociation>									<containmentAssociation	className="gov.nih.nci.dynext.pathology_specimen.SpecimenHistologicType" cardinality="*" roleName="histologicType" parentRoleName="specimenBaseSolidTissuePathologyAnnotation">					<attribute name="type" csvColumnName="Histoogic Type" />										<containmentAssociation	className="gov.nih.nci.dynext.pathology_specimen.SpecimenHistologicVariantType"	parentRoleName="histologicType" cardinality="*" roleName="histologicVariantType" >						<attribute name="otherHistologicType" csvColumnName="Histologic Variant Type" />					</containmentAssociation>						</containmentAssociation>							<containmentAssociation	className="gov.nih.nci.dynext.pathology_specimen.SpecimenHistologicGrade"	parentRoleName="specimenBaseSolidTissuePathologyAnnotation"  cardinality="*" roleName="histologicGrade">					<attribute name="gradingSystemName" csvColumnName="Grading System Name" />					<attribute name="grade" csvColumnName="Grade" />					<attribute name="otherGradingSystemName" csvColumnName="Other Grading System Name" />				</containmentAssociation>					</containmentAssociation>		</containmentAssociation>				<attribute name="label" csvColumnName="Label" updateBasedOn="true"/>		<attribute name="activityStatus" csvColumnName="Specimen Activity Status"/>	</BulkOperationClass></BulkOperationMetadata>
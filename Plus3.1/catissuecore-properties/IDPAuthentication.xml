<?xml version="1.0" encoding="UTF-8"?>

<IDPS>
	<IDP name="CSM">
		<class name="edu.wustl.authmanager.CSMAuthManager" />
		<idpdetails>
		<defaultidp>true</defaultidp>
		<displayname>CSM</displayname>
		</idpdetails>
		<idp-configuration>
		</idp-configuration>
        <user-attributes>
		</user-attributes>
		<migration-details>
			<migrator>
				<rule-class></rule-class>
			    <migrator-class>edu.wustl.migrator.WUSTLKeyMigrator</migrator-class>
				<target-idp-name>WUSTLKEY_IDP</target-idp-name>
			</migrator>
		</migration-details>
	</IDP>
	<IDP name="WUSTLKEY_IDP">
		        <class name="edu.wustl.authmanager.LDAPAuthManager" />
        <idpdetails>
            <defaultidp>false</defaultidp>
            <displayname>Utah</displayname>
        </idpdetails>
        <idp-configuration>
            <initContextFactory>com.sun.jndi.ldap.LdapCtxFactory</initContextFactory>
            <securityAuthetication>simple</securityAuthetication>
            <securityProtocol></securityProtocol>
            <directory>ldap://ldap.testathon.net:389/</directory>
            <searchbase>ou=Users,dc=testathon,dc=net</searchbase>
            <searchfilter>CN=Users</searchfilter>
            <stringToReplace>Users</stringToReplace>
            <bindUser>cn=stuart,ou=Users,dc=testathon,dc=net</bindUser>
            <bindPassword>stuart</bindPassword>
        </idp-configuration>
        <user-attributes>
            <firstName>givenName</firstName>
            <lastName>sn</lastName>
        </user-attributes>
        <migration-details>
		</migration-details>
    </IDP>
</IDPS>

<?xml version="1.0" encoding="UTF-8"?>
<BulkOperationMetadata templateName="createDNADerivatives" batchSize="5">
	<BulkOperationClass className="edu.wustl.catissuecore.domain.Specimen">
		<referenceAssociation
					className="edu.wustl.catissuecore.domain.Specimen" 
					roleName="parentSpecimen"  parentRoleName="specimen" >
					<attribute name="label" csvColumnName="Parent Label"   />
		</referenceAssociation>
		<containmentAssociation
			className="edu.wustl.catissuecore.domain.SpecimenPosition"
			  
			 roleName="specimenPosition"   >
				<attribute name="positionDimensionOneString" csvColumnName="Position One"   />
				<attribute name="positionDimensionTwoString" csvColumnName="Position Two"   />
				<referenceAssociation
				className="edu.wustl.catissuecore.domain.StorageContainer"
				  
				 roleName="storageContainer"    >
					<attribute name="name" csvColumnName="Sample Box Name"/>
				</referenceAssociation>			
		</containmentAssociation>

		<attribute name="tissueSite" csvColumnName="Tissue Site" defaultValue="Not Specified" />
		<attribute name="tissueSide" csvColumnName="Tissue Side" defaultValue="Not Specified" />
		<attribute name="label" csvColumnName="Label"/>
		<attribute name="createdOn" csvColumnName="Extraction Date" />
		<attribute name="activityStatus" csvColumnName="Activity Status" defaultValue="Active"/>
		<attribute name="comment" csvColumnName="Comments" />
		<attribute name="isAvailable" csvColumnName="Is Available" />
		<attribute name="collectionStatus" csvColumnName="Collection Status" defaultValue="Collected" />
		<attribute name="specimenClass" csvColumnName="Specimen Class" defaultValue="Molecular" >

		</attribute>
		<attribute name="specimenType" csvColumnName="Type" defaultValue="DNA" />
		<attribute name="initialQuantity" csvColumnName="DNA Quantity" />
		<attribute name="availableQuantity" csvColumnName="DNA Quantity" />
		<attribute name="lineage" csvColumnName="Lineage" defaultValue="Derived" />
		<attribute name="pathologicalStatus" csvColumnName="Pathological Status" defaultValue="Not Specified" />
		<attribute name="concentrationInMicrogramPerMicroliter" csvColumnName="Concentration"   />
		<attribute name="thawCycleIncrementBy" csvColumnName="Freeze thaw increment"   />		
		<attribute name="dnaMethod" csvColumnName="Method" defaultValue="Not Specified" />
		<attribute name="dna260" csvColumnName="260-280" />

	</BulkOperationClass>
</BulkOperationMetadata>

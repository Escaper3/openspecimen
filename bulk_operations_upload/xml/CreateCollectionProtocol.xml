<?xml version="1.0" encoding="UTF-8" standalone="no"?><BulkOperationMetadata batchSize="5" templateName="CreateCollectionProtocol">
	<BulkOperationClass className="edu.wustl.catissuecore.domain.CollectionProtocol">

		<referenceAssociation className="edu.wustl.catissuecore.domain.User" roleName="principalInvestigator">
				<attribute csvColumnName="PrincipalInvestigator" name="loginName"/>
		</referenceAssociation>

		<referenceAssociation cardinality="*" className="edu.wustl.catissuecore.domain.User" roleName="coordinatorCollection">
				<attribute csvColumnName="Principalcordinator" name="loginName"/>
		</referenceAssociation>
		
		<containmentAssociation cardinality="*" className="edu.wustl.catissuecore.domain.ConsentTier" maxNoOfRecords="4" roleName="consentTierCollection">
				<attribute csvColumnName="Consent" name="statement"/>
		</containmentAssociation>

		<containmentAssociation cardinality="*" className="edu.wustl.catissuecore.domain.ClinicalDiagnosis" parentRoleName="collectionProtocol" roleName="clinicalDiagnosisCollection">
				<attribute csvColumnName="ClinicalDiagnosis" name="name"/>	
		</containmentAssociation>
		
		<referenceAssociation cardinality="*" className="edu.wustl.catissuecore.domain.Site" roleName="siteCollection">
				<attribute csvColumnName="SiteID" name="id"/>
		</referenceAssociation>

		<containmentAssociation cardinality="*" className="edu.wustl.catissuecore.domain.CollectionProtocolEvent" maxNoOfRecords="2" parentRoleName="collectionProtocol" roleName="collectionProtocolEventCollection">

			<attribute csvColumnName="Study Calender Event Point" name="studyCalendarEventPoint"/>								
			<attribute csvColumnName="CollectionPointLabel" name="collectionPointLabel"/>

			<attribute csvColumnName="Clinical Status" name="clinicalStatus"/>		
			<attribute csvColumnName="ActivityStatus" name="activityStatus"/>


			<containmentAssociation cardinality="*" className="edu.wustl.catissuecore.domain.SpecimenRequirement" maxNoOfRecords="2" parentRoleName="collectionProtocolEvent" roleName="specimenRequirementCollection">

				<attribute csvColumnName="Specimen Class" name="specimenClass">

					<!-- <discriminatorCollection name="Cell" value="edu.wustl.catissuecore.domain.CellSpecimenRequirement"/>
					<discriminatorCollection name="Molecular" value="edu.wustl.catissuecore.domain.MolecularSpecimenRequirement"/>
					<discriminatorCollection name="Fluid" value="edu.wustl.catissuecore.domain.FluidSpecimenRequirement"/>
					<discriminatorCollection name="Tissue" value="edu.wustl.catissuecore.domain.TissueSpecimenRequirement"/> -->

				</attribute>

				<attribute csvColumnName="Specimen Type" name="specimenType"/>
				<attribute csvColumnName="Storage Location" name="storageType"/>
				<attribute csvColumnName="Pathological Status" name="pathologicalStatus"/>
				<attribute csvColumnName="Initial Quantity" name="initialQuantity"/>
				<attribute csvColumnName="Lineage" name="lineage"/>
				<attribute csvColumnName="LabelFormat" name="labelFormat"/>

				<referenceAssociation className="edu.wustl.catissuecore.domain.SpecimenCharacteristics" roleName="specimenCharacteristics">
						<attribute csvColumnName="Tissue Site" name="tissueSite"/>
						<attribute csvColumnName="Tissue Side" name="tissueSide"/>
				</referenceAssociation>

				<containmentAssociation cardinality="*" className="edu.wustl.catissuecore.domain.CollectionEventParameters" parentRoleName="specimen" roleName="specimenEventCollection">
						<referenceAssociation className="edu.wustl.catissuecore.domain.User" roleName="user">
							<attribute csvColumnName="CUserID" name="id"/>
						</referenceAssociation>
						<attribute csvColumnName="CComments" name="comment"/>								
						<attribute csvColumnName="Procedure" name="collectionProcedure"/>
						<attribute csvColumnName="Container" name="container"/>
						<attribute csvColumnName="CollectedDate(MM/DD/YYYY)" name="timestamp"/>
				</containmentAssociation>

				<containmentAssociation cardinality="*" className="edu.wustl.catissuecore.domain.ReceivedEventParameters" parentRoleName="specimen" roleName="specimenEventCollection">
						<referenceAssociation className="edu.wustl.catissuecore.domain.User" roleName="user">
							<attribute csvColumnName="RUserID" name="id"/>
						</referenceAssociation>
						<attribute csvColumnName="RComments" name="comment"/>								
						<attribute csvColumnName="Quality" name="receivedQuality"/>				
						<attribute csvColumnName="ReceivedDate(MM/DD/YYYY)" name="timestamp"/>
				</containmentAssociation>

			</containmentAssociation>

		</containmentAssociation>

		<attribute csvColumnName="Title" name="title"/>
		<attribute csvColumnName="ShortTitle" name="shortTitle"/>
		<attribute csvColumnName="IRB" name="irbIdentifier"/>
		<attribute csvColumnName="Date" name="startDate"/>
		<attribute csvColumnName="URL" name="descriptionURL"/>
		<attribute csvColumnName="Activity Status" name="activityStatus"/>
		<attribute csvColumnName="Waived" name="consentsWaived"/>
		<attribute csvColumnName="Aliquot In Same Container" name="aliquotInSameContainer"/>
		<attribute csvColumnName="Enrollment" name="enrollment"/>

		<attribute csvColumnName="AliquotLabelFormat" name="aliquotLabelFormat"/>
		<attribute csvColumnName="DerivativeLabelFormat" name="derivativeLabelFormat"/>
		<attribute csvColumnName="SpecimenLabelFormat" name="specimenLabelFormat"/>
	</BulkOperationClass>
</BulkOperationMetadata>
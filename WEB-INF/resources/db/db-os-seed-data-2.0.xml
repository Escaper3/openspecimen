		<changeSet author="vgaikwad" id="sequen1ce" dbms="oracle">
    <createSequence 
      sequenceName="RBAC_OPERATION_SEQ" 
      startValue="1" 
      incrementBy="1" 
      ordered="true" />
  </changeSet>
  
  <changeSet author="vgaikwad" id="sequence" dbms="oracle">
    <createSequence 
      sequenceName="RBAC_RESOURCES_SEQ" 
      startValue="1" 
      incrementBy="1" 
      ordered="true" />
  </changeSet>
<changeSet author="vgaikwad" id="Add operations" dbms="oracle">
    <sql>
       insert into RBAC_OPERATIONS(IDENTIFIER, NAME) values (RBAC_OPERATION_SEQ.nextval, 'test1');
       insert into RBAC_RESOURCES(IDENTIFIER, NAME) values(RBAC_RESOURCES_SEQ.nextval, 'test1');
    </sql>
  </changeSet>      
</databaseChangeLog>
<object-schema>
  <name>cpr</name>

  <record> 
    <name>com.krishagni.catissueplus.core.biospecimen.events.ParticipantRegistrationsList</name>
    
    <record>
      <attribute>participant</attribute>
   
      <field>
        <caption>First Name</caption>
        <attribute>firstName</attribute>
      </field>     

      <field>
        <caption>Last Name</caption>
        <attribute>lastName</attribute>
      </field>     

      <field>
        <caption>Middle Name</caption>
        <attribute>middleName</attribute>
      </field>     

      <field>
        <type>date</type>
        <caption>Date Of Birth</caption>
        <attribute>birthDate</attribute>
      </field>  

      <field>
        <type>date</type>
        <caption>Date Of Death</caption>
        <attribute>deathDate</attribute>
      </field>  

      <field>
        <caption>Gender</caption>
        <attribute>gender</attribute>
      </field>  

      <field>
        <multiple>true</multiple>
        <caption>Race</caption>
        <attribute>races</attribute>
      </field>  

      <field>
        <caption>Vital Status</caption>
        <attribute>vitalStatus</attribute>
      </field>  

      <field>
        <caption>Ethnicity</caption>
        <attribute>ethnicity</attribute>
      </field>  

      <field>
        <caption>SSN</caption>
        <attribute>uid</attribute>
      </field>  

      <field>
        <caption>EMPI</caption>
        <attribute>empi</attribute>
      </field>  

      <field>
        <caption>Activity Status</caption>
        <attribute>activityStatus</attribute>
      </field>  

      <record>
        <caption>PMI</caption>
        <attribute>pmis</attribute>
        <multiple>true</multiple>

        <field>
          <caption>Site Name</caption>
          <attribute>siteName</attribute>
        </field>

        <field>
          <caption>MRN</caption>
          <attribute>mrn</attribute>
        </field>
      </record>
    </record>

    <record>
      <caption>CPR</caption>
      <attribute>registrations</attribute>
      <multiple>true</multiple>

      <field>
        <caption>CP Short Title</caption>
        <attribute>cpShortTitle</attribute>
      </field>

      <field>
        <caption>PPID</caption>
        <attribute>ppid</attribute>
      </field>

      <field>
        <type>date</type>
        <caption>Registration Date</caption>
        <attribute>registrationDate</attribute>
      </field>
    </record>
  </record>
</object-schema>

<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.administrative.domain.DistributionProtocolRequirement"
      table="OS_DISTRIBUTION_PROTOCOL_REQUIREMENTS">

    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">OS_DISTRIBUTION_PROTOCOL_REQUIREMENTS_SEQ</param>
      </generator>
    </id>
    
    <many-to-one
      name="dp"
      cascade="none"
      class="com.krishagni.catissueplus.core.administrative.domain.DistributionProtocol"
      column="DISTRIBUTION_PROTOCOL_ID" />
      
    <property name="specimenType" column="SPECIMEN_TYPE" />
    
    <property name="anatomicSite" column="ANATOMIC_SITE" />
    
    <property name="pathologyStatus" column="PATHOLOGY_STATUS" />
    
    <property name="specimenRequired" column="SPECIMEN_REQUIRED" />
    
    <property name="price" column="PRICE" />
    
    <property name="comments" column="COMMENTS" />
    
    <property name="activityStatus" column="ACTIVITY_STATUS" />
    
    <filter name="activeEntity" />
  </class>
</hibernate-mapping>

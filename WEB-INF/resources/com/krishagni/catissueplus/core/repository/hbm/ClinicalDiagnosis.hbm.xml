<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class
    name="com.krishagni.catissueplus.core.biospecimen.domain.ClinicalDiagnosis"
    table="CATISSUE_CLINICAL_DIAGNOSIS"
    lazy="true">
    
    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">CATISSUE_CLINICAL_DIAG_SEQ</param>           
      </generator>
    </id>

    <property name="name" column="CLINICAL_DIAGNOSIS"/>
    
    <many-to-one name="collectionProtocol" cascade="none" column="COLLECTION_PROTOCOL_ID"/>
  </class>
</hibernate-mapping>


<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping auto-import="false">
    <class
        name="com.krishagni.catissueplus.core.extapp.domain.ExternalAppNotification"
        table="CATISSUE_NOTIFICATION_STATUS"
	lazy="false" >
    
	<cache usage="read-write" />
	
	 <id name="id"  column="IDENTIFIER"	>
    <generator class="foreign">
      <param name="property">audit</param>
    </generator>
  </id>
  <one-to-one name="audit" class="com.krishagni.catissueplus.core.audit.domain.Audit" constrained="true"/>
		
        <many-to-one
           name="externalApplication"
           class="com.krishagni.catissueplus.core.extapp.domain.ExternalApplication"
           cascade="save-update"
           column="APP_ID"/>
                   
        <property 
		name="status" 
		type="string" 
		column="STATUS" 
		 />

        <property
            name="comments"
            type="string"
            column="COMMENTS"/>
            
            
    </class>
</hibernate-mapping>

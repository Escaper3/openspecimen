<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- test check-in -->
<hibernate-mapping auto-import="false">
  <class 
    name="com.krishagni.catissueplus.core.biospecimen.domain.SpecimenRequirement" 
    table="CATISSUE_CP_REQ_SPECIMEN">

    <id
      name="id"
      column="IDENTIFIER"
      type="long"
      length="30"
      unsaved-value="null">
      
      <generator class="native">
        <param name="sequence">CATISSUE_REQ_SPECIMEN_SEQ</param>
      </generator>
    </id>

    <property
      name="collectionTimestamp"
      type="timestamp"
      column="COLLECTION_TIMESTAMP"
      length="50"/>

    <many-to-one
      name="collector"
      class="com.krishagni.catissueplus.core.administrative.domain.User"
      cascade="none"
      column="COLLECTOR_ID"/>

    <property
      name="collectionComments"
      type="string"
      column="COLLECTION_COMMENTS"
      length="2000"/>

    <property
      name="collectionProcedure"
      type="string"
      column="COLLECTION_PROCEDURE"
      length="50"/>

    <property
      name="collectionContainer"
      type="string"
      column="COLLECTION_CONTAINER"
      length="50"/>

    <property
      name="receivedQuality"
      type="string"
      column="RECEIVED_QUALITY"
      length="50"/>

    <property
      name="receivedTimestamp"
      type="timestamp"
      column="RECEIVED_TIMESTAMP"
      length="50"/>

    <many-to-one
      name="receiver"
      class="com.krishagni.catissueplus.core.administrative.domain.User"
      cascade="none"
      column="RECEIVER_ID"/>

    <property
      name="receivedComments"
      type="string"
      column="RECEIVED_COMMENTS"
      length="2000"/>

    <property
      name="storageType"
      type="string"
      column="STORAGE_TYPE"
      length="50"/>

    <many-to-one
      name="collectionProtocolEvent"
      class="com.krishagni.catissueplus.core.biospecimen.domain.CollectionProtocolEvent"
      cascade="none"
      column="COLLECTION_PROTOCOL_EVENT_ID"/>

    <property
      name="labelFormat"
      type="string"
      column="LABELFORMAT"
      length="255" />

    <property
      name="specimenRequirementLabel"
      type="string"
      column="SPEC_REQ_LABEL"
      length="255"/>

    <property
      name="activityStatus"
      type="string"
      column="ACTIVITY_STATUS"
      length="50"/>
      
    <property
      name="initialQuantity"
      type="double"
      column="INITIAL_QUANTITY"
      length="50"/>

    <property
      name="pathologicalStatus"
      type="string"
      column="PATHOLOGICAL_STATUS"
      length="50"/>

    <property
      name="lineage"
      type="string"
      column="LINEAGE"
      length="50"/>
      
    <property
      name="specimenClass"
      type="string"
      column="SPECIMEN_CLASS"
      length="50"/>

    <property
      name="specimenType"
      type="string"
      column="SPECIMEN_TYPE"
      length="50"/>
      
    <property
      name="tissueSide"
      type="string"
      column="TISSUE_SIDE"
      length="50"/>

    <property
      name="tissueSite"
      type="string"
      column="TISSUE_SITE"
      length="50"/>
      
    <many-to-one
      name="parentSpecimenRequirement"
      class="com.krishagni.catissueplus.core.biospecimen.domain.SpecimenRequirement"
      cascade="none"
      update="true"
      column="PARENT_SPECIMEN_ID"
      lazy="false"/>
      
    <set
      name="specimens"
      table="CATISSUE_SPECIMEN"
      lazy="true"
      inverse="true"
      cascade="all"
      sort="unsorted">

      <key column="PARENT_SPECIMEN_ID"/>
      <one-to-many class="com.krishagni.catissueplus.core.biospecimen.domain.SpecimenRequirement"/>
    </set>
  </class>
</hibernate-mapping>


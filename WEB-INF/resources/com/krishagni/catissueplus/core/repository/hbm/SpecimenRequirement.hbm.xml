<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<!-- test check-in -->
<hibernate-mapping auto-import="false">
  <class 
    name="com.krishagni.catissueplus.core.biospecimen.domain.SpecimenRequirement" 
    table="CATISSUE_CP_REQ_SPECIMEN">

    <id name="id" column="IDENTIFIER">
      <generator class="native">
        <param name="sequence">CATISSUE_REQ_SPECIMEN_SEQ</param>
      </generator>
    </id>

    <property name="collectionTimestamp" column="COLLECTION_TIMESTAMP"/>

    <many-to-one name="collector" cascade="none" column="COLLECTOR_ID"/>

    <property name="collectionComments" column="COLLECTION_COMMENTS"/>

    <property name="collectionProcedure" column="COLLECTION_PROCEDURE"/>

    <property name="collectionContainer" column="COLLECTION_CONTAINER"/>

    <property name="receivedQuality" column="RECEIVED_QUALITY"/>

    <property name="receivedTimestamp" column="RECEIVED_TIMESTAMP"/>

    <many-to-one name="receiver" cascade="none" column="RECEIVER_ID"/>

    <property name="receivedComments" column="RECEIVED_COMMENTS"/>

    <property name="storageType" column="STORAGE_TYPE"/>

    <many-to-one name="collectionProtocolEvent" cascade="none" column="COLLECTION_PROTOCOL_EVENT_ID"/>

    <property name="labelFormat" column="LABELFORMAT"/>

    <property name="specimenRequirementLabel" column="SPEC_REQ_LABEL"/>

    <property name="activityStatus" column="ACTIVITY_STATUS"/>
      
    <property name="initialQuantity" column="INITIAL_QUANTITY"/>

    <property name="pathologicalStatus" column="PATHOLOGICAL_STATUS"/>

    <property name="lineage" column="LINEAGE"/>
      
    <property name="specimenClass" column="SPECIMEN_CLASS"/>

    <property name="specimenType" column="SPECIMEN_TYPE"/>
      
    <property name="tissueSide" column="TISSUE_SIDE"/>

    <property name="tissueSite" column="TISSUE_SITE"/>
      
    <set name="specimens" table="CATISSUE_SPECIMEN" inverse="true" cascade="all">
      <key column="PARENT_SPECIMEN_ID"/>
      <one-to-many class="com.krishagni.catissueplus.core.biospecimen.domain.SpecimenRequirement"/>
    </set>
  </class>
</hibernate-mapping>

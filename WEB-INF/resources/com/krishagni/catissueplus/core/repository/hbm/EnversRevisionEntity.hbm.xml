<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
  <class name="com.krishagni.catissueplus.core.auditquery.domain.EnversRevisionEntity"
    table="REVINFO">

    <id name="id" column="REV">
      <generator class="native">
        <param name="sequence">OS_AUDIT_QUERY_SEQ</param>
      </generator>
    </id>

    <property name="revtstmp" column="REVTSTMP" />

    <property name="userId" column="USER_ID" />
    
    <property name="ipAddress" column="IP_ADDRESS" />
    
    <set name="entityNames" table="REVCHANGES">
      <key column="REV"/>
      <element column="ENTITYNAME" type="string"></element>
    </set>
  </class>

</hibernate-mapping>
        
        
<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false">
	<class name="com.krishagni.catissueplus.core.auditquery.domain.EnversRevisionEntity"
		table="REVINFO">

		<id name="id" column="REV" type="long" length="30">
			<generator class="native">
				<param name="sequence">CATISSUE_AUDIT_QUERY_SEQ</param>
			</generator>
		</id>

		<property name="revtstmp" type="timestamp" column="REVTSTMP" />

		<property name="userId" type="long" column="USER_ID" />
		
		<property name="ipAddress" type="string" column="IP_ADDRESS" />
	</class>

</hibernate-mapping>
        
        
			<stringToReplace>Users</stringToReplace>
			<bindUser>uid=admin,ou=system</bindUser>
			<bindPassword>secret</bindPassword>
		</LDAP-configuration>
		<error-codes>
		</error-codes>
		<user-attributes>
			<firstName>givenname</firstName>
			<lastName>sn</lastName>
		</user-attributes>
	</domain>

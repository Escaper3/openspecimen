<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    
<hibernate-mapping
>
    <class
        name="edu.wustl.catissuecore.domain.SpecimenProtocol"
        table="CATISSUE_SPECIMEN_PROTOCOL"
		lazy="true"
    >

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_SPECIMEN_PROTOCOL_SEQ</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-SpecimenProtocol.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <many-to-one
            name="principalInvestigator"
            class="edu.wustl.catissuecore.domain.User"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="PRINCIPAL_INVESTIGATOR_ID"
        />

        <property
            name="title"
            type="string"
            update="true"
            insert="true"
            column="TITLE"
            length="255"
            not-null="true"
            unique="true"
        />

        <property
            name="shortTitle"
            type="string"
            update="true"
            insert="true"
            column="SHORT_TITLE"
            length="255"
        />

        <property
            name="irbIdentifier"
            type="string"
            update="true"
            insert="true"
            column="IRB_IDENTIFIER"
            length="255"
        />

        <property
            name="startDate"
            type="date"
            update="true"
            insert="true"
            column="START_DATE"
            length="50"
        />

        <property
            name="endDate"
            type="date"
            update="true"
            insert="true"
            column="END_DATE"
            length="50"
        />

        <property
            name="enrollment"
            type="int"
            update="true"
            insert="true"
            column="ENROLLMENT"
            length="50"
        />

        <property
            name="descriptionURL"
            type="string"
            update="true"
            insert="true"
            column="DESCRIPTION_URL"
            length="255"
        />

        <property
            name="activityStatus"
            type="string"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS"
            length="50"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-SpecimenProtocol.xml
            containing the additional properties and place it in your merge dir.
        -->

        <joined-subclass
            name="edu.wustl.catissuecore.domain.CollectionProtocol"
            table="CATISSUE_COLLECTION_PROTOCOL"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />
        <property
            name="unsignedConsentDocumentURL"
            type="string"
            update="true"
            insert="true"
            column="UNSIGNED_CONSENT_DOC_URL"
            length="1000"
        />


        <set
            name="collectionProtocolRegistrationCollection"
            table="CATISSUE_COLL_PROT_REG"
            lazy="true"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="COLLECTION_PROTOCOL_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.CollectionProtocolRegistration"
              />
        </set>

        <set
            name="consentTierCollection"
            table="CATISSUE_CONSENT_TIER"
            lazy="true"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="COLL_PROTOCOL_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.ConsentTier"
              />
        </set>

        <set
            name="distributionProtocolCollection"
            table="CATISSUE_COLL_DISTRIBUTION_REL"
            lazy="true"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="COLLECTION_PROTOCOL_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.DistributionProtocol"
                column="DISTRIBUTION_PROTOCOL_ID"
                outer-join="auto"
             />

        </set>

        <set
            name="coordinatorCollection"
            table="CATISSUE_COLL_COORDINATORS"
            lazy="true"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

            <key
                column="COLLECTION_PROTOCOL_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.User"
                column="USER_ID"
                outer-join="auto"
             />

        </set>

        <set
            name="collectionProtocolEventCollection"
            table="CATISSUE_COLL_PROT_EVENT"
            lazy="true"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="COLLECTION_PROTOCOL_ID"
            >
            </key>

            <one-to-many
                  class="edu.wustl.catissuecore.domain.CollectionProtocolEvent"
            />	 
    </set>


		<property
            name="aliquotInSameContainer"
            type="boolean"
            update="true"
            insert="true"
            column="ALIQUOT_IN_SAME_CONTAINER"
        />

        <property
            name="consentsWaived"
            type="boolean"
            update="true"
            insert="true"
            column="CONSENTS_WAIVED"
        />

        </joined-subclass>
        
        <joined-subclass
            name="edu.wustl.catissuecore.domain.DistributionProtocol"
            table="CATISSUE_DISTRIBUTION_PROTOCOL"
			lazy="true"
        >
            <key
                column="IDENTIFIER"
            />

        <set
            name="specimenRequirementCollection"
            table="CATISSUE_DISTRIBUTION_SPE_REQ"
            lazy="true"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="DISTRIBUTION_PROTOCOL_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.SpecimenRequirement"
                column="SPECIMEN_REQUIREMENT_ID"
                outer-join="auto"
             />

        </set>

        <set
            name="collectionProtocolCollection"
            table="CATISSUE_COLL_DISTRIBUTION_REL"
            lazy="true"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

            <key
                column="DISTRIBUTION_PROTOCOL_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.CollectionProtocol"
                column="COLLECTION_PROTOCOL_ID"
                outer-join="auto"
             />

        </set>

        </joined-subclass>

		    <joined-subclass
            name="edu.wustl.catissuecore.domain.ClinicalStudy"
            table="CATISSUE_CLINICAL_STUDY"
            lazy="true"
        >
        <key
            column="IDENTIFIER"
        />

        <set
            name="coordinatorCollection"
            table="CATISSUE_CLINICAL_STUDY_COORDINATORS"
            lazy="true"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >

              <key
                  column="CLINICAL_STUDY_ID"
              />

              <many-to-many
                  class="edu.wustl.catissuecore.domain.User"
                  column="USER_ID"
                  outer-join="auto"
              />

        </set>
	<set
            name="clinicalStudyEventCollection"
            table="CATISSUE_CLINICAL_STUDY_EVENT"
            lazy="true"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="CLINICAL_STUDY_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.ClinicalStudyEvent"
              />
        </set>

 	  <set
            name="clinicalStudyRegistrationCollection"
            table="CATISSUE_CLINICAL_STUDY_REG"
            lazy="true"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >

              <key
                  column="CLINICAL_STUDY_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.ClinicalStudyRegistration"
              />
        </set>

        </joined-subclass>

    </class>

</hibernate-mapping>

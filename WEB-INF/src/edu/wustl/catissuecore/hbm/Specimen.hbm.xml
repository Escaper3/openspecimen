<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping
>
    <class
        name="edu.wustl.catissuecore.domain.Specimen"
        table="CATISSUE_SPECIMEN"
		lazy="true"
    >
    
    <cache usage="read-write" />

        <id
            name="id"
            column="IDENTIFIER"
            type="long"
            length="30"
            unsaved-value="null"
        >
            <generator class="native">
                <param name="sequence">CATISSUE_SPECIMEN_SEQ</param>
              <!--  
                  To add non XDoclet generator parameters, create a file named 
                  hibernate-generator-params-Specimen.xml 
                  containing the additional parameters and place it in your merge dir. 
              --> 
            </generator>
        </id>

        <discriminator
            column="SPECIMEN_CLASS"
        />

	 <set
            name="consentTierStatusCollection"
            table="CATISSUE_CONSENT_TIER_STATUS"
            lazy="true"
            inverse="false"
            cascade="save-update"
            sort="unsorted"
        >
        
        <cache usage="read-write" />

              <key
                  column="SPECIMEN_ID"
              />

              <one-to-many
                  class="edu.wustl.catissuecore.domain.ConsentTierStatus"
              />
        </set>

        <property
            name="type"
            type="string"
            update="true"
            insert="true"
            column="TYPE"
            length="50"
        />
			<one-to-one
            name="specimenPosition"
	            class="edu.wustl.catissuecore.domain.SpecimenPosition"
	            cascade="save-update"
	            outer-join="auto"
	            constrained="false"
		     	property-ref="specimen"
		     	
	       />
        <property
            name="available"
            type="boolean"
            update="true"
            insert="true"
            column="AVAILABLE"
        />

       <property
            name="positionDimensionOne"
            type="int"
            update="true"
            insert="true"
            column="POSITION_DIMENSION_ONE"
            length="30"
        />

        <property
            name="barcode"
            type="string"
            update="true"
            insert="true"
            column="BARCODE"
            length="255"
            unique="true"
        />

        <property
            name="comment"
            type="string"
            update="true"
            insert="true"
            column="COMMENTS"
            length="2000"
        />

        <property
            name="activityStatus"
            type="string"
            update="true"
            insert="true"
            column="ACTIVITY_STATUS"
            length="50"
        />

        <property
            name="collectionStatus"
            type="string"
            update="true"
            insert="true"
            column="COLLECTION_STATUS"
            length="50"
        />
        
		<property
            name="createdOn"
            type="date"
            update="true"
            insert="true"
            column="CREATED_ON_DATE"
        />

		<property
            name="isCollectionProtocolRequirement"
            type="boolean"
            update="true"
            insert="true"
            column="IS_COLL_PROT_REQ"
        />
        
        <many-to-one
            name="parentSpecimen"
            class="edu.wustl.catissuecore.domain.Specimen"
            cascade="none"
            outer-join="false"
            update="true"
            insert="true"
            column="PARENT_SPECIMEN_ID"  
            lazy="false"
        />

        <set
            name="biohazardCollection"
            table="CATISSUE_SPECIMEN_BIOHZ_REL"
            lazy="false" fetch="subselect"
            inverse="false"
            cascade="none"
            sort="unsorted"
        >
        
        <cache usage="read-write" />

            <key
                column="SPECIMEN_ID"
            >
            </key>

            <many-to-many
                class="edu.wustl.catissuecore.domain.Biohazard"
                column="BIOHAZARD_ID"
                outer-join="auto"
             />

        </set>

        <set
            name="specimenEventCollection"
            table="CATISSUE_SPECIMEN_EVENT"
            lazy="false" fetch="subselect"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >
        
        <cache usage="read-write" />

            <key
                column="SPECIMEN_ID"
            >
            </key>

            <one-to-many
                  class="edu.wustl.catissuecore.domain.SpecimenEventParameters"
            />

        </set>

        <set
            name="childrenSpecimen"
            table="CATISSUE_SPECIMEN"
            lazy="true"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >
        
        <cache usage="read-write" />

            <key
                column="PARENT_SPECIMEN_ID"
            >
            </key>

            <one-to-many
                  class="edu.wustl.catissuecore.domain.Specimen"
            />

        </set>

        <set
            name="externalIdentifierCollection"
            table="CATISSUE_EXTERNAL_IDENTIFIER"
            lazy="false" fetch="subselect"
            inverse="true"
            cascade="save-update"
            sort="unsorted"
        >
        
        <cache usage="read-write" />

            <key
                column="SPECIMEN_ID"
            >
            </key>

            <one-to-many
                  class="edu.wustl.catissuecore.domain.ExternalIdentifier"
            />

        </set>

        <many-to-one
            name="specimenCollectionGroup"
            class="edu.wustl.catissuecore.domain.AbstractSpecimenCollectionGroup"
            cascade="none"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_COLLECTION_GROUP_ID"
            lazy="false"
        />


        <many-to-one
            name="specimenCharacteristics"
            class="edu.wustl.catissuecore.domain.SpecimenCharacteristics"
            cascade="none"
            lazy="false"
            outer-join="auto"
            update="true"
            insert="true"
            column="SPECIMEN_CHARACTERISTICS_ID"
        />

        <component
            name="availableQuantity"
            class="edu.wustl.catissuecore.domain.Quantity"  lazy="false" 
        >

        <property
            name="value"
            type="double"
            update="true"
            insert="true"
            column="AVAILABLE_QUANTITY"
            length="50"
        />

        </component>

        <component
            name="initialQuantity"
            class="edu.wustl.catissuecore.domain.Quantity" lazy="false" 
        >

        <property
            name="value"
            type="double"
            update="true"
            insert="true"
            column="QUANTITY"
            length="50"
        />

        </component>

        <property
            name="pathologicalStatus"
            type="string"
            update="true"
            insert="true"
            column="PATHOLOGICAL_STATUS"
            length="50"
        />

        <property
            name="label"
            type="string"
            update="true"
            insert="true"
            column="LABEL"
            length="255"
        />

        <property
            name="lineage"
            type="string"
            update="true"
            insert="true"
            column="LINEAGE"
            length="50"
        />

        <!--
            To add non XDoclet property mappings, create a file named
                hibernate-properties-Specimen.xml
            containing the additional properties and place it in your merge dir.
        -->
        <subclass
            name="edu.wustl.catissuecore.domain.FluidSpecimen"
            discriminator-value="Fluid"
			lazy="true"
        >

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-FluidSpecimen.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.CellSpecimen"
            discriminator-value="Cell"
			lazy="true"
        >

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-CellSpecimen.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.MolecularSpecimen"
            discriminator-value="Molecular"
			lazy="true"
        >
        <property
            name="concentrationInMicrogramPerMicroliter"
            type="double"
            update="true"
            insert="true"
            column="CONCENTRATION"
            length="50"
        />

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-MolecularSpecimen.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        <subclass
            name="edu.wustl.catissuecore.domain.TissueSpecimen"
            discriminator-value="Tissue"
			lazy="true"
        >

	    <!--
            	To add non XDoclet property mappings, create a file named
                hibernate-properties-TissueSpecimen.xml
		containing the additional properties and place it in your merge dir.
	    -->

        </subclass>
        

    </class>

</hibernate-mapping>
